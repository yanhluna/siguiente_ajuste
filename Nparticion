/**
 *
 * @author yanhm
 */
public class Nparticion{
    
    int id;
    int tamano;
    boolean ocupada;
    String nombreNproces;
    
    public Nparticion(int id, int tamano){
        
        this.id=id;
        this.tamano=tamano;
        this.ocupada=false;
        this.nombreNproces="";
        
    }
    
}
